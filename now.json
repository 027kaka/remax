{
  "version": 2,
  "name": "remax-website",
  "builds": [
    {
      "src": "website/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/website/dist/index.html",
      "continue": true
    }
  ]
}
