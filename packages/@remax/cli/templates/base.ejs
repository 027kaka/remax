<template name="REMAX_TPL">
  <block a:for="{{$$REMAX_ROOT}}" a:key="">
    <template is="REMAX_TPL_1" data="{{item}}"/>
  </block>
</template>

<template name="REMAX_TPL_1">
  <block>
    <block a:if="{{item && item.text}}">
      <!-- string -->
      {{item.text}}
    </block>

    <%- include('./component.ejs', {
      type: 'view',
      propKeys: [],
      id: 'view',
    }) %>

    <% for (let component of components) { %>
      <%- include('./component.ejs', {
        type: component.type,
        propKeys: component.propKeys,
        id: component.id,
      }) %>
    <% } %>
  </block>
</template>
