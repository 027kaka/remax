<template name="REACT_MINI_APP_TPL">
    <block wx:for="{{$$REACT_MINI_APP_ROOT}}" wx:key="">
        <template is="REACT_MINI_APP_TPL_1" data="{{item}}"/>
    </block>
</template>



<% for (let i = 0; i < 8; i ++) { %>
<template name="REACT_MINI_APP_TPL_<%=i%>">
    <block wx:if="{{item.text}}">
      <!-- string -->
      {{item.text}}
    </block>

    <%- include('./component.wxml.ejs', {
        type: 'view',
        tpl_index: i,
        propKeys: [
            'hover-class',
            'hover-stop-propagation',
            'hover-start-time',
            'hover-stay-time',
        ]
    }) %>
  
    <%- include('./component.wxml.ejs', {
        type: 'image',
        tpl_index: i,
        propKeys: [
            'src',
            'mode',
            'lazy-load',
            'binderror',
            'bindload',
        ]
    }) %>

    <%- include('./component.wxml.ejs', {
        type: 'button',
        tpl_index: i,
        propKeys: [
            'size',
            'type',
            'plain',
            'disabled',
            'loading',
            'form-type',
            'open-type',
            'hover-class',
            'hover-stop-propagation',
            'hover-start-time',
            'hover-stay-time',
            'lang',
            'bindgetuserinfo',
            'session-from',
            'send-message-title',
            'send-message-path',
            'send-message-img',
            'show-message-card',
            'bindcontact',
            'bindgetphonenumber',
            'app-parameter',
            'binderror',
            'bindopensetting',
        ]
    }) %>

</template>
<% } %>
