<%_ if (id === 'swiper') { _%>
<block s-for="i.children">
  <swiper-item item-id="{{i.nodes[item].props.itemId}}">
    <block s-for="i.nodes[item].children" s-for-item="sItem">
      <template is="{{'REMAX_TPL_' + (tid + 1) + '_CONTAINER'}}" data="{{{i: i.nodes[item].nodes[sItem], an: an, tid: tid + 1 }}}" />
    </block>
  </swiper-item>
</block>
<%_ } else if (type === 'native') { _%>
<block s-for="i.children">
  <block s-if="i.nodes[item].props['slot']">
    <view slot="{{i.nodes[item].props['slot']}}" <% for(let i=0;i < slotView.props.length; i++) { %>
      <%=slotView.props[i]%>="{{i.nodes[item].props['<%=slotView.props[i]%>']}}" <%}%>
    >
      <block s-for="i.nodes[item].children" s-for-item="sItem">
        <template is="{{'REMAX_TPL_' + (tid + 1) + '_CONTAINER'}}" data="{{{i: i.nodes[item].nodes[sItem], an: an, tid: tid + 1 }}}" />
      </block>
    </view>
  </block>
  <block s-else>
    <template is="{{'REMAX_TPL_' + (tid + 1) + '_CONTAINER'}}" data="{{{i: i.nodes[item], an: an, tid: tid + 1 }}}" />
  </block>
</block>
<%_ } else { _%>
<block s-for="i.children">
  <template style="{{i.nodes[item].props.style}}" class="{{i.nodes[item].props.class}}" is="{{'REMAX_TPL_' + (tid + 1) + '_CONTAINER'}}" data="{{{i: i.nodes[item], an: an, tid: tid + 1 }}}" />
</block>
<%_ } _%>
